(\"Etes vous sure ?\");\n    if (!c) return;\n    console.log(client);\n      this.adminClientService.deleteResource(client.mat).subscribe(data => { \n        if (this.clients.length <= 1 && this.clients.length >= 0) {\n          this.show=false\n          \n        }\n      this.onGetAllClients();\n   \n    }, err => {\n        console.log(err);\n    })\n  }\n  onNewClient(){\n    this.mode = 'new-cat';\n  }\n  onSaveClient(data){\n    let url = this.adminClientService.host2 + \"/client\" ;\n    this.adminClientService.postResource(url,data)\n\n    .subscribe(data => {\n      this.mode= 'list' ;\n      this.onGetAllClients();\n      \n      \n      \n    },err => {\n        console.log(err);\n    })\n  }\n  currentClient : Client= {\n    \n    id  : ' ' ,\n    mat : ' ' ,\n    username : ' ' ,\n    cin : ' ' ,\n    phoneNumber : ' ' ,\n    password : ' ' ,\n    address : ' ' \n    \n}\n  onEditClient(data){\n    console.log(data);\n    \n    this.adminClientService.getResource(data.mat)\n   \n    .subscribe(client => {\n   \n      this.currentClient = client; \n  \n      console.log(this.currentClient)\n   \n    }, err => {\n      console.log(err);\n    })\n  }\n\n  onUpdateClient (data) { \n    this.adminClientService.putResource(this.currentClient.mat , data )\n    .subscribe(data => {\n      console.log(\"omar\");\n      this.mode = \"list\";\n      this.onGetAllClients(); \n      \n    }, err => {\n      console.log(err);\n    })\n  }\n  \n  }\n","<app-admin-side-nav></app-admin-side-nav>\n<div class=\"col main pt-5 mt-3\" style=\"position: absolute; top: 5px; left: 15%; width: 80%;\">\n<div class=\"panel panel-default\">\n   \n    <div class=\"panel-body\">\n        <div>\n            <button  class=\"btn btn-info btn-lg\" (click)=\"onNewClient()\">\n                <span class=\"glyphicon glyphicon-plus\"></span> Add New User \n            </button>\n        </div>\n        <div *ngIf=\"mode == 'new-cat' \" class=\"col-md-6\">\n            <form #f=\"ngForm\"